<% if current_user.admin? %>
<h1> <%= @applicant.name %></h1>
<p>First Name: <%= @applicant.name %></p>
<p>Last Name: <%= @applicant.last_name %></p>
<p>Age: <%= @applicant.age %></p>
<p>Applicant Address: <%= @applicant.full_address %></p>

<p>Education: <%= @applicant.education %></p>
<p>Applicant ID: <%= @applicant.aid %></p>
<% else %>
<h1><%= render 'layouts/oops' %></h1>
<% end %>

<% if current_user == nil %>
<h1><%= render 'layouts/oops' %></h1>
<% else %>
<section class='login'>
<h1> Students matching "<%=@results%>"</h1>

<% if @final_results != nil %>
<% @final_results.each do |res| %>
<hr>
<p><%= link_to "#{res.full_name}", course_student_path(res.course_id, res.id) %></p><br>
<% end %>
<% end %>
<% end %>
<hr>
<br><br><br>
<section>
